{
  "name": "@chernodub/typescript-boilerplate",
  "version": "1.1.0",
  "description": "The typescript boilerplate with no prod dependencies",
  "private": "true",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run build-dev && concurrently \"npm:watch-dev\" \"npm:serve-dev\"",
    "serve-dev": "live-server --ignorePattern=\".*\\.ts\" ./dist",
    "watch-dev": "npm run build-dev -- -w",
    "build": "webpack",
    "build-dev": "webpack --mode development --devtool inline-source-map"
  },
  "keywords": [],
  "author": "Viktor Chernodub",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^4.5.0",
    "live-server": "^1.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "dependencies": {}
}
